.content{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
 background-repeat: repeat;
 background-color:darkgray;
 padding-bottom: 240rpx;
} 
.small{
  font-size: smaller;
  margin-left: 10rpx;
  margin-right: 130rpx;
}
#swCall120{
  position: absolute;
  left: 80%;
  top:2%;
}
#swmessage{
  position: absolute;
  left: 80%;
  top:14%;
}
.choice{
  height: 150rpx;
  margin-bottom: 10rpx;
  width: 95%;
  border-bottom-style: solid;
  border-bottom-color: white;
  border-width: 2rpx;
}
.auto{
  width: 95%;
}
textarea{
  border-style: solid;
  border-color: gray;
  border-width: 2rpx;
  border-radius: 2rpx;
  height: 260rpx;
  width: 600rpx;
}
button{
  box-shadow: 2rpx,2rpx,2rpx,2rpx;
  color: red;
  font-size: small;
}
.edit{
  margin-top: 50rpx;
}
.contactInput{
   border-style: solid;
  border-color: gray;
  border-width: 2rpx;
  border-radius: 2rpx;
  height: 80rpx;
  width: 600rpx;
}